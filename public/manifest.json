{
  "name": "Garik",
  "description": "Shows Igor Guberman's short verse (garik) on a new tab.",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  }
}